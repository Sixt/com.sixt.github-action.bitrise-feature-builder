{
  "name": "bitrise-feature-builder",
  "version": "0.1.0",
  "private": true,
  "description": "Trigger feature builds on bitrise by commenting on a PR",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "lint": "eslint */*.ts",
    "package": "ncc build --source-map",
    "all": "npm run build && npm run format && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sixt/com.sixt.github-action.bitrise-feature-builder.git"
  },
  "keywords": [
    "actions",
    "node",
    "bitrise",
    "trigger"
  ],
  "author": "Sixt SE",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@actions/io": "^1.0.2",
    "@octokit/core": "^3.2.1",
    "@octokit/plugin-paginate-rest": "^2.6.0",
    "@octokit/plugin-rest-endpoint-methods": "4.2.1",
    "@octokit/webhooks": "^7.11.4",
    "@types/node": "^14.11.2",
    "axios": "^0.21.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@zeit/ncc": "^0.20.5",
    "jest": "^26.4.2",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.13.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
